<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bodytrack" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BodyTrack" >
        <activity
            android:name=".View.CadastroSerie"
            android:exported="true" />
        <activity android:name=".Control.ListviewAdapter" />
        <activity android:name=".View.CadastroAtividade" />
        <activity android:name=".View.CadastroTreino" />
        <activity android:name=".View.Home" />
        <activity android:name=".View.Cadastro" />
        <activity android:name=".View.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>